@page
@model SportsStore.Pages.CompletedModel
@{
    Layout = "_CartLayout";
    ViewBag.Title = "Order completed";
}

<div class="text-center">
    <h2>Thanks!</h2>
    <p>Thanks for placing order #@Model.OrderId</p>
    <p>We'll ship your goods as soon as possible.</p>
    <a class="btn btn-primary" asp-page="Index">Return to Store</a>
</div>
